{
  "name": "component-store",
  "version": "0.0.1",
  "description": "Store for Componento components.",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "build": "npm run build-ts",
    "serve": "node dist/index.js",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"nodemon dist/index.js\"",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/componento/component-store.git"
  },
  "author": "inspired consulting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/componento/component-store/issues"
  },
  "homepage": "https://github.com/componento/component-store#readme",
  "dependencies": {
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^13.13.4",
    "concurrently": "^5.2.0",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
